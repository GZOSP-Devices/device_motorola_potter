service camera-provider-2-4 /vendor/bin/hw/android.hardware.camera.provider@2.4-service_moto
    class hal
    user cameraserver
    group audio camera input drmrpc
    ioprio rt 4
    capabilities SYS_NICE
    writepid /dev/cpuset/camera-daemon/tasks /dev/stune/top-app/tasks
    setenv LD_PRELOAD "/vendor/lib/libshim_camera_hal.so"
